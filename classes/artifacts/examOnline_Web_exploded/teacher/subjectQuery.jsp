                    <div class="widgetcontent">
                        <form class="stdform" action="querySubjectLike.action" method="get">

                            <label>试题题目:</label>
                            <span class="field"><input type="text" name="subjectTitle" class="input-xlarge"
                                                       placeholder="请输入..."/></span>

                            <p class="stdformbutton">
                                <input type="submit" name="Submit" value="查询">
                                <input type="reset" name="Submit2" value="重置">
                            </p>

                        </form>
                    </div><!--widgetcontent-->
                </div><!--widget-->

                <br/><br/>


            </div><!--maincontentinner-->
        </div><!--maincontent-->

</body>
</html>
	